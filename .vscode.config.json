{
    "diffEditor.ignoreTrimWhitespace": false,
    "diffEditor.renderSideBySide": true,
    "editor.formatOnSave": true,
    "extensions.ignoreRecommendations": false,
    "files.exclude": {
        "**/built": true,
        "**/.parcel-cache": true
    },
    "files.insertFinalNewline": true,
    "html.format.wrapLineLength": 150,
    "search.exclude": {
        "**/built": true,
        "**/.parcel-cache": true,
        "**/node_modules": true
    },
    "terminal.integrated.cursorStyle": "line",
    "terminal.integrated.fontSize": 16,
    "terminal.integrated.inheritEnv": false,
    "terminal.integrated.scrollback": 100000,
    "workbench.colorCustomizations": {
        "activityBar.activeBackground": "#004541",
        "activityBar.background": "#001d2f",
        "activityBar.foreground": "#00ffff",
        "activityBar.inactiveForeground": "#ffffff",
        "activityBarBadge.background": "#146b97",
        "activityBarBadge.foreground": "#00ffff",
        "tab.activeBackground": "#003633",
        "tab.activeForeground": "#00ffff",
        "tab.activeBorder": "#00ffff",
        "tab.activeBorderTop": "#00ffff",
        "tab.activeModifiedBorder": "#ff00ff",
        "terminal.background": "#001c26",
        "terminal.foreground": "#002c53",
        "terminalCursor.background": "#220051",
        "terminalCursor.foreground": "#a939a2",
        "terminal.ansiBlack": "#3d3d3d",
        "terminal.ansiBlue": "#00339a",
        "terminal.ansiBrightBlack": "#000000",
        "terminal.ansiBrightBlue": "#0066ff",
        "terminal.ansiBrightCyan": "#00ffff",
        "terminal.ansiBrightGreen": "#00ff00",
        "terminal.ansiBrightMagenta": "#ff009d",
        "terminal.ansiBrightRed": "#ff0000",
        "terminal.ansiBrightWhite": "#ffffff",
        "terminal.ansiBrightYellow": "#ffff00",
        "terminal.ansiCyan": "#00a5a5",
        "terminal.ansiGreen": "#2a9f00",
        "terminal.ansiMagenta": "#870053",
        "terminal.ansiRed": "#ab1400",
        "terminal.ansiWhite": "#dddddd",
        "terminal.ansiYellow": "#cacd00"
    },
    "workbench.colorTheme": "Dracula",
    "workbench.startupEditor": "newUntitledFile"
}
